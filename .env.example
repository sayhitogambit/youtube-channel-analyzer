# IPRoyal Residential Proxies Configuration
# Get your credentials from: https://dashboard.iproyal.com/

# IPRoyal Proxy Credentials (REQUIRED for most actors)
IPROYAL_USERNAME=your_username_here
IPROYAL_PASSWORD=your_password_here

# Optional: Geographic targeting (append to password)
# Examples:
# IPROYAL_PASSWORD=your_password_country-us
# IPROYAL_PASSWORD=your_password_country-us_state-california
# IPROYAL_PASSWORD=your_password_country-gb_city-london

# IPRoyal Settings
IPROYAL_HOST=geo.iproyal.com
IPROYAL_PORT=12321
IPROYAL_PROTOCOL=http

# IPRoyal API Key (for API access, different from proxy auth)
IPROYAL_API_KEY=4208f4caae4b821ba392d6c60c7dcc51b45c4de0e3c299c4dd0c152f1cc3

# Proxy Rotation Strategy
PROXY_ROTATION=smart  # Options: round_robin, random, smart

# Rate Limiting (adjust based on your IPRoyal plan)
RATE_LIMIT_REQUESTS=30
RATE_LIMIT_WINDOW=60

# Output Settings
OUTPUT_DIR=output
CACHE_ENABLED=true
CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO
